<h1>Listing insurances</h1>

<table>
  <thead>
    <tr>
      <th>Insurance</th>
      <th>Insuranceaddress</th>
      <th>Insurancecity</th>
      <th>Insurancestate</th>
      <th>Insurancezip</th>
      <th>Insurancephone</th>
      <th>Prrname</th>
      <th>Prrphone</th>
      <th>Prrext</th>
      <th>Prrfax</th>
      <th>Prremail</th>
      <th>Ncrname</th>
      <th>Ncrphone</th>
      <th>Nvrext</th>
      <th>Ncvrfax</th>
      <th>Ncremail</th>
      <th>Crname</th>
      <th>Crphone</th>
      <th>Crext</th>
      <th>Crfax</th>
      <th>Cremail</th>
      <th>Insurancenotes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @insurances.each do |insurance| %>
      <tr>
        <td><%= insurance.insurance %></td>
        <td><%= insurance.insuranceaddress %></td>
        <td><%= insurance.insurancecity %></td>
        <td><%= insurance.insurancestate %></td>
        <td><%= insurance.insurancezip %></td>
        <td><%= insurance.insurancephone %></td>
        <td><%= insurance.prrname %></td>
        <td><%= insurance.prrphone %></td>
        <td><%= insurance.prrext %></td>
        <td><%= insurance.prrfax %></td>
        <td><%= insurance.prremail %></td>
        <td><%= insurance.ncrname %></td>
        <td><%= insurance.ncrphone %></td>
        <td><%= insurance.nvrext %></td>
        <td><%= insurance.ncvrfax %></td>
        <td><%= insurance.ncremail %></td>
        <td><%= insurance.crname %></td>
        <td><%= insurance.crphone %></td>
        <td><%= insurance.crext %></td>
        <td><%= insurance.crfax %></td>
        <td><%= insurance.cremail %></td>
        <td><%= insurance.insurancenotes %></td>
        <td><%= link_to 'Show', insurance %></td>
        <td><%= link_to 'Edit', edit_insurance_path(insurance) %></td>
        <td><%= link_to 'Destroy', insurance, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Insurance', new_insurance_path %>
