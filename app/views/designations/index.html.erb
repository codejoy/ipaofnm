<h1>Listing designations</h1>

<table>
  <thead>
    <tr>
      <th>Designation</th>
      <th>Dues</th>
      <th>Amtchangedate</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @designations.each do |designation| %>
      <tr>
        <td><%= designation.designation %></td>
        <td><%= designation.dues %></td>
        <td><%= designation.amtchangedate %></td>
        <td><%= designation.status %></td>
        <td><%= link_to 'Show', designation %></td>
        <td><%= link_to 'Edit', edit_designation_path(designation) %></td>
        <td><%= link_to 'Destroy', designation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Designation', new_designation_path %>
